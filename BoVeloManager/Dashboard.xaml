<Window x:Class="BoVeloManager.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoVeloManager"
        mc:Ignorable="d"
        Title="Dashboard" Height="450" Width="800">
    <DockPanel>
        <Grid>
            <StackPanel Background="#FF3E3E3E">
                <Button Tag="Catalog"  x:Name="BTCatalog" Width="60" Height="50" Click="BTPage_Click">Catalog</Button>
                <Button Width="60" Height="50">Stock</Button>
                <Button Width="60" Height="50">Command</Button>
                <Button Width="60" Height="50">Planning</Button>
                <Button Width="60" Height="50">Managment</Button>
            </StackPanel>
        </Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden">
            
        </Frame>
    </DockPanel>
</Window>

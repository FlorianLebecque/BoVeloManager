<Window x:Class="BoVeloManager.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoVeloManager"
        mc:Ignorable="d"
        Title="Dashboard" Height="450" Width="800">
    <DockPanel>
        <Grid>
            <StackPanel Width="100" x:Name="sidebar">
                <Button Tag="Catalog"  x:Name="BTCatalog" Height="25" Click="BTPage_Click">Catalog</Button>
                <Button Tag="Catalog" Height="25" Click="BTPage_Click">Stock</Button>
                <Button Tag="Catalog" Height="25" Click="BTPage_Click">Command</Button>
                <Button Tag="Catalog" Height="25" Click="BTPage_Click">Planning</Button>
                <Button Tag="Catalog" Height="25" Click="BTPage_Click">Managment</Button>
            </StackPanel>
        </Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden">
            
        </Frame>
         
    </DockPanel>
</Window>
